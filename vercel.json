{
    "trailingSlash": false,
    "builds": [
        {
            "src": "main.go",
            "use": "@vercel/go"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/main.go"
        }
    ],
    "functions": {
        "main.go": {
            "runtime": "@vercel/go",
            "maxDuration": 50
        }
    }
}
